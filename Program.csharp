using System; // main library

class Program_Uno //program
{
    static int Factorial(int n) => (n == 0 || n == 1) ? 1 : n * Factorial(n - 1); // factorial function with one line

    static void Main(string[] args)
    {
        int num = 5; // Change the number to calculate factorial
        int result = Factorial(num);
        Console.WriteLine($"Factorial of {num} is: {result}");
    }
}
